	partition dCalendario = m
		mode: import
		source = ```
				let
				    // Insira a data de referência aqui
				    MenorData = List.Min(fPonto[data_id]),
				    MaiorData = List.Max(fPonto[data_id]),
				    
				    //Não alterar daqui pra baixo	
				    
				    DataInicio = Date.StartOfYear(MenorData),
				    DataFim = Date.EndOfYear(MaiorData), 
				    Dias = Duration.Days(DataFim - DataInicio) + 1,
				    ListarDatas = List.Dates(DataInicio, Dias, #duration(1,0,0,0)),
				    
				    Tabela = #table(
				        type table[
				            Data = date,
				            Ano = Int64.Type,
				            NomeMes = text,
				            MesAbre = text,
				            MesAno = text,
				            MesNum = number,
				            AnoMesINT = number,
				            InicioMes = date,
				            Trimestre = Int64.Type,
				            TrimestreAbreviado = text,
				            Bimestre = text,
				            Semestre = text,
				            Semana = Int64.Type,
				            DiaSemana = Int64.Type,
				            NomeDia = text,
				            Passado = logical,
				            AnoAtual = text,
				            MesAtual = text,
				            Dia = Int64.Type,
				            DiaDeTrabalho = text,
				            DiaAtual = text,
				            AnoMovel = Int64.Type,
				            MesMovel = Int64.Type,
				            DiaMovel = Int64.Type
				        ],
				        List.Transform(
				            ListarDatas,
				            each let
				                currentDate = _,
				                currentYear = Date.Year(currentDate),
				                currentMonth = Date.Month(currentDate),
				                currentDay = Date.Day(currentDate),
				                today = Date.From(DateTime.LocalNow()) // Obtém a data atual
				            in
				            {
				                currentDate,
				                currentYear,
				                Text.Proper( Date.MonthName(currentDate, "pt-BR")),
				                Text.Proper(Text.Start(Date.MonthName(currentDate, "pt-BR"), 3)),
				                Text.Proper(Text.Start(Date.MonthName(currentDate, "pt-BR"), 3)) & "-" & Text.End(Text.From(currentYear), 2),
				                currentMonth,
				                currentYear * 100 + currentMonth,
				                Date.StartOfMonth(currentDate),
				                Date.QuarterOfYear(currentDate),
				                Text.From(Date.QuarterOfYear(currentDate)) & "º Trim",
				                Text.From( Number.RoundUp( currentMonth/2,0)) & "º Bim",
				                Text.From( Number.RoundUp( currentMonth/6,0)) & "º Sem",
				                Date.WeekOfMonth(currentDate),
				                Date.DayOfWeek(currentDate, Day.Monday), // Usando Day.Monday para o cálculo do Dia da Semana
				                Date.DayOfWeekName(currentDate, "pt-BR"),
				                if currentDate <= MaiorData then true else false,
				                if Date.IsInCurrentYear(currentDate) then "Ano Atual" else Text.From(currentYear),
				                if Date.IsInCurrentYear(currentDate) and Date.IsInCurrentMonth(currentDate) then "Mês Atual" else Text.Proper( Date.MonthName(currentDate, "pt-BR")),
				                
				                
				                currentDay, // Dia
				                if Date.DayOfWeek(currentDate, Day.Monday) = 6 or Date.DayOfWeek(currentDate, Day.Monday) = 7 then "N" else "S", // DiaDeTrabalho (Sab/Dom = N)
				                if Date.IsInCurrentDay(currentDate) then "S" else "N", // DiaAtual
				                Date.Year(today) - currentYear, // AnoMovel
				                (Date.Year(today) - currentYear) * 12 + (Date.Month(today) - currentMonth), // MesMovel
				                Duration.Days(today - currentDate) // DiaMovel
				            }
				        )
				    )
				in
				    Tabela
				```

	annotation PBI_NavigationStepName = Navegação

