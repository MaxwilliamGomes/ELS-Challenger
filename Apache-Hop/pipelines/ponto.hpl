// 2. Limpeza do FuncionarioID (se tiver hífen, limpamos o campo original)
if (funcID_texto != null &amp;&amp; funcID_texto.indexOf("-") !== -1) {
    FuncionarioID = null; 
} else {
    // 3. Devolvemos o valor limpo (sem pontos) para o campo original
    FuncionarioID = funcID_texto;
}</jsScript_script>
